int led1 = D7;

void setup() 
{

	pinMode(led1, OUTPUT);

}

void loop()
{

  //E .
	digitalWrite(led1, HIGH);     
	delay(500);

	digitalWrite(led1, LOW);
	delay(1500);
	
  //Z --..
  digitalWrite(led1, HIGH);    
	delay(1500);
	
	digitalWrite(led1, LOW);
	delay(500);
	
	digitalWrite(led1, HIGH);    
	delay(1500);
	
	digitalWrite(led1, LOW);
	delay(500);
	
	digitalWrite(led1, HIGH);
	delay(500);
	
	digitalWrite(led1, LOW);
	delay(500);
	
	digitalWrite(led1, HIGH);
	delay(500);
	
	digitalWrite(led1, LOW);
	delay(1500);
	
	//E .
	digitalWrite(led1, HIGH);     
	delay(500);

	digitalWrite(led1, LOW);
	delay(1500);
	
	//K -.-
  digitalWrite(led1, HIGH);    
	delay(1500);
	
	digitalWrite(led1, LOW);
	delay(500);
	
	digitalWrite(led1, HIGH);
	delay(500);
	
	digitalWrite(led1, LOW);
	delay(500);
	
	digitalWrite(led1, HIGH);    
	delay(1500);
	
	digitalWrite(led1, LOW);
	delay(1500);
	
	//I ..
	digitalWrite(led1, HIGH);
	delay(500);
	
	digitalWrite(led1, LOW);
	delay(500);
	
	digitalWrite(led1, HIGH);
	delay(500);
	
	digitalWrite(led1, LOW);
	delay(1500);
	
	//E .
	digitalWrite(led1, HIGH);     
	delay(500);

	digitalWrite(led1, LOW);
	delay(1500);
	
	//L .-..
	digitalWrite(led1, HIGH);
	delay(500);
	
	digitalWrite(led1, LOW);
	delay(500);
	
  digitalWrite(led1, HIGH);    
	delay(1500);
	
	digitalWrite(led1, LOW);
	delay(500);
	
	digitalWrite(led1, HIGH);
	delay(500);
	
	digitalWrite(led1, LOW);
	delay(500);
	
	digitalWrite(led1, HIGH);
	delay(500);
	
	digitalWrite(led1, LOW);
	delay(3500);
}
